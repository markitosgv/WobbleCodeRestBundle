{
    "name": "wobblecode/rest-bundle",
    "type": "symfony-bundle",
    "description": "A bundle that creates REST Apis based on your current controllers actions",
    "keywords": ["symfony", "rest", "restful", "api", "content negotiation"],
    "homepage": "https://github.com/wobblecode/WobbleCodeRestBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Hdez",
            "email": "luis@wobblecode.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/dependency-injection": "~2.3|~3.0",
        "willdurand/negotiation": "2.0.2",
        "doctrine/common": "~2.2"
    },
    "require-dev": {
        "symfony/yaml": "*",
        "symfony/browser-kit": "*",
        "sensio/framework-extra-bundle": "*",
        "symfony/class-loader": "*",
        "symfony/finder": "*",
        "symfony/css-selector": "*",
        "symfony/process": "*",
        "doctrine/doctrine-bundle": "*",
        "symfony/twig-bundle": "*",
        "doctrine/orm": "*",
        "symfony/form": "*",
        "symfony/validator": "*",
        "jms/serializer-bundle": "~1.1.0"
    },
    "suggest": {
        "jms/serializer-bundle": "~1.1.0"
    },
    "autoload": {
        "psr-4": { "WobbleCode\\RestBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Fixtures\\": "Tests/Fixtures/" }
    },
    "minimum-stability": "dev",
    "target-dir": "WobbleCode/RestBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
